{
  "extends": ["config:base"],
  "enabledManagers": ["regex"],
  "customManagers": [
    {
      "managerFilePatterns": ["^apps/.+/renovate-latest.yml$"],
      "matchStrings": [
        "name:\\s*(?<depName>\\S+)"
      ],
      "datasourceTemplate": "helm",
      "versioningTemplate": "helm"
    }
  ]
}
